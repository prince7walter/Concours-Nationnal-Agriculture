 String sql = "INSERT INTO \"CONCOURS\".\"USERS\" VALUES ('Konan Harvey', 'harvey', 'harvey@dba.com', 'harvey')";
String sql = "INSERT INTO \"CONCOURS\".\"USERS\" (NOM, PSEUDO, EMAIL, PASS) VALUES (?, ?, ?, ?)";
        int ok = 0 ;
        try {
           // db.initSimp(sql);
            db.initPrepar(sql);
            db.getPstm().setString(1,"Kamo Rogrique");
            db.getPstm().setString(2, "kamo");
            db.getPstm().setString(3, "kamo@bda.com");
            db.getPstm().setString(4, "kamo");

            ok = db.executeMaj();
        } catch (Exception ex){
            ex.printStackTrace();
        }

SELECT
String sql ="SELECT * FROM users";
        try {
            db.initPrepar(sql);

            ResultSet rs = db.executeSelect();
            while (rs.next()){
                System.out.print(rs.getString(1));
                System.out.print(rs.getString(2));
                System.out.print(rs.getString(3));
                System.out.print(rs.getString(4));
                System.out.println("");
            }
        } catch (Exception ex){
            ex.printStackTrace();
        }


