       private void addPieceType()
           {
               ObservableList<PieceType> pieceTypes = FXCollections.observableArrayList();
               List<PieceType> pclist = pieceTypeM.get();
               for (PieceType p : pclist){
                   pieceTypes.add(p);
               }
               combo_piece.setItems(pieceTypes);
           }

